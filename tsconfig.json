{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "suppressImplicitAnyIndexErrors": false
  },
  "exclude": [
    "node_modules",
    ".quasar",
    "dist"
  ]
}
